// Unique across multiple CSF1 subcats + customs.
// Baseline: ...|B|<csf1Id>|<attrNum>
// Custom:   ...|C|0|<customNum>
export const rowKey = (r) => {
  const org = r.orgID ?? '0';
  const sub = r.csF2SubcategoryID ?? '0';
  const ml  = r.maturityLevel ?? '0';
  const isCustom = !!(r.isCustom || r.customAttributeDefinition || r.customAttributeNum != null);

  if (isCustom) {
    return `${org}|${sub}|${ml}|C|0|${r.customAttributeNum ?? '0'}`;
  }
  // baseline
  const csf1 = r.csF1SubcategoryID ?? '0';
  return `${org}|${sub}|${ml}|B|${csf1}|${r.attributeNum ?? '0'}`;
};
