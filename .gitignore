 {compactMode && (() => {
       const st = statusBySub.get(s.id);
       const when = st?.at ? new Date(st.at).toLocaleString() : '';
       const tip =
         st?.state === 'Completed'
           ? `Completed${st.by ? ' by ' + st.by : ''}${when ? ' on ' + when : ''}`
           : st?.state === 'Reopened'
           ? `Reopened${st.by ? ' by ' + st.by : ''}${when ? ' on ' + when : ''}`
           : 'Not complete';
       return (
         <Box
           sx={{ position: 'absolute', top: 6, right: 6 }}
           onClick={(e) => e.stopPropagation()}
           onMouseDown={(e) => e.stopPropagation()}
         >
           <Tooltip title={tip} placement="top" arrow enterDelay={200}>
             <span>{renderStatusChip(st)}</span>
           </Tooltip>
         </Box>
       );
     })()}
